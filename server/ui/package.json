{
  "name": "validator-frontend",
  "version": "0.0.0",
  "private": true,
  "apiProxy": "http://localhost:8080",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "cross-env NODE_ENV=development docusaurus start",
